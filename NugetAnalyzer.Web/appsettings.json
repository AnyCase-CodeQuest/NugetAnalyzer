{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "ConnectionString": {
    "DefaultConnection": "Data Source=.;Initial Catalog=NugetAnalyzerDB;Integrated Security=True;"
  },
  "NugetEndpoints": {
    "Search": "https://api-v2v3search-1.nuget.org",
    "PackageMetadata": "https://api.nuget.org"
  },
  "PackageStatus": {
    "VersionStatus": {
      "Major": "Error",
      "Minor": "Warning",
      "Build": "Info",
      "Revision": "Info"
    },
    "DateBordersInMonths": {
      "WarningBottomBorder": "6",
      "ErrorBottomBorder": "12"
    },
    "ObsoleteBorderInMonths": "12"
  },
  "GitHubEndPoints": {
    "AuthorizationEndpoint": "https://github.com/login/oauth/authorize",
    "TokenEndpoint": "https://github.com/login/oauth/access_token",
    "UserInformationEndpoint": "https://api.github.com/user"
  },
  "Directories": {
    "RepositoryCloneDirectory": "tempRepositoriesCloning" 
  },
  "AllowedHosts": "*",
  "ApplicationName": "GodelTechnologiesNugetAnalyzer"  
}
