<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <link rel="Shortcut Icon" href="~/images/logo/logo.ico" type="image/x-icon" />
    <title>@ViewData["Title"]</title>

    <link href="~/stylesheets/bootstrap.min.css" rel="stylesheet" />

    <link href="~/stylesheets/Shared/SignIn.min.css" rel="stylesheet" />
</head>
<body>
    @RenderBody()
    <footer class="container">
        <div class="row justify-content-center align-items-center">
            <div class="footer__link">
                <a asp-action="About" asp-controller="Home">
                    About
                </a>
            </div>
            <div class="footer__link">
                <a asp-action="Contacts" asp-controller="Home">
                    Contacts
                </a>
            </div>
        </div>
            
        
    </footer>
    <script src="~/scripts/jquery-3.4.0.min.js"></script>
    <script src="~/scripts/popper.min.js"></script>
    <script src="~/scripts/bootstrap.min.js"></script>
</body>
</html>
